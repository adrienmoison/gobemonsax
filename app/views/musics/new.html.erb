<%= form_for @music do |f| %>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>
  <div class="form-group">
    <%= f.label :track, "enter a trackname" %>
    <%= f.text_field :track %>
  </div>
  <div class="form-group">
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </div>
  <div class="form-group">
    <%= f.label :category %>
    <%= f.text_field :category %>
  </div>
    <%= f.submit class: 'btn btn-primary' %>
<% end %>

<%= link_to "Back to list", musics_path %>
