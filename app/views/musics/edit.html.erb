<%= form_for @music do |f| %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :track %>
  <%= f.text_field :track %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>

<%= link_to "Back to list", musics_path %>
